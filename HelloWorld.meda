fn do_something_really_cool {
    fn assert(condition: bool) {
        if !condition { panic }
    }
    fn remainder_three_numbers(a: i32, b: i32, c: i32): i32 {
        a % b % c
    }
    foo := remainder_three_numbers(1, 2, 3)
    foo = foo + 2

    fn print_u8_string(str: u8*) {
        print(str)
    }

    fn print_i8_string(str: i8*) {
        print(str)
    }

    print_u8_string("Hello, unsigned world!")
    print_i8_string("Hello, signed world! (again)")
}
