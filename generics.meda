fn memset[T](dest: T *mut, src: T, count: usize) {
    cur := dest
    end :: (dest as usize + size_of(T) * count) as T *mut
    while cur as usize != end as usize {
        *dest = src
        cur = (cur as usize + size_of(T)) as T *mut
    }
}

fn main() {
    buffer :: malloc(24) as u8 *mut
    shit(*buffer)
    memset(buffer, 34, 24)
    //memset[u8](buffer, 34, 24)
}

